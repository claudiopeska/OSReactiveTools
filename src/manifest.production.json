{
    "manifest_version": 2,
    "name": "OS Reactive Tools",
    "description": "OS Reactive Tools",
    "version": "0.0.1",
    "icons": {
        "48": "assets/icon.png"
    },
    "devtools_page": "devtools.html",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "*://*/*",
        "https://*/",
        "http://*/",
		"debugger"
    ]
}